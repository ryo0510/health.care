<div class="container-fluid">
	<h2 class="section-top">コース結果登録</h2>
	<div class="col-md-3 col-sm-12 col-xs-12">
   <%= form_with model: @course_result, url: course_result_path(@course_result), local: true do |f| %>
      <div class="title">
        <%= f.label :history %>
        <%= f.text_field :history %>
      </div>
    <div class="time">
    <%= f.label :start_time %>
    <%= f.date_select :start_time %>
  </div>

  <div class="content">
    <%= f.label :memo %>
    <%= f.text_field :memo %>
  </div>


  <div class="submit">
    <%= f.submit %>
  </div>

   <% end %>
  </div>
 </div>